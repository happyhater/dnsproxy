{
  "Vendor": true,
  "Test": true,
  "Deadline": "2m",
  "Sort": ["linter", "severity", "path", "line"],
  "Exclude": [
    ".*generated.*"
  ],
  "EnableGC": true,
  "Linters": {
    "nakedret": {
      "Command": "nakedret",
      "Pattern": "^(?P<path>.*?\\.go):(?P<line>\\d+)\\s*(?P<message>.*)$"
    }
  },
  "WarnUnmatchedDirective": false,
  "EnableAll": true,
  "DisableAll": false,
  "Cyclo": 16,
  "LineLength": 200,
  "Disable": [
    "maligned"
  ]
}